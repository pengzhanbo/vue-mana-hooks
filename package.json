{
  "name": "vue-mana-hooks",
  "version": "1.0.0",
  "description": "Vue3 hooks",
  "homepage": "https://github.com/pengzhanbo/vue-mana-hooks",
  "repository": {
    "url": "https://github.com/pengzhanbo/vue-mana-hooks",
    "type": "git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "dev": "vite",
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true,
    "dts": true
  },
  "prettier": "@pengzhanbo/prettier-config",
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^0.3.11",
    "@pengzhanbo/prettier-config": "^0.3.11",
    "@types/node": "^18.16.5",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
